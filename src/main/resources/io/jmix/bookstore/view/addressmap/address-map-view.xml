<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view"
      xmlns:maps="http://jmix.io/schema/maps/ui"
      title="msg://addressMapView.title">
    <layout>
        <maps:geoMap width="40em" height="30em" id="map">
            <maps:layers>
                <maps:tile>
                    <maps:osmSource/>
                </maps:tile>
                <maps:vector id="addressLayer">
                    <maps:vectorSource id="addressSource"/>
                </maps:vector>
            </maps:layers>
            <maps:mapView centerX="-99.755859" centerY="39.164141" zoom="4.0"/>
        </maps:geoMap>
    </layout>
</view>